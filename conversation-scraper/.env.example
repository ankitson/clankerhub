# Claude.ai session cookie (get from browser cookies when logged into claude.ai)
# Starts with "sk-ant-sid01-..."
CLAUDE_SESSION_KEY=

# (Optional) Organization UUID - auto-detected if not set
CLAUDE_ORG_ID=

# (Optional) Path to Claude Code data directory
# Default: ~/.claude
CLAUDE_CODE_DIR=

# (Optional) Output directory for scraped data
# Default: ./data (relative to project root)
OUTPUT_DIR=

# (Optional) Auto-commit changes to git after scraping
AUTO_COMMIT=false
